/* pages/Mine/couponManagement/couponManagement.wxss */
page {
	background-color: #f3f3f3;
}

.coupon_rule {
	background-color: #fff;
	font-size: 28rpx;
	padding: 10rpx;
	text-align: center;
}

.delete_img {
	width: 40rpx;
}

.coupon_list {
	padding: 30rpx;
	padding-bottom: 150rpx;
}

.coupon_item {
	padding: 30rpx 20rpx;
	background-color: #fff;
	border-radius: 10rpx;
	margin: 20rpx 0;
}

.coupon_item_name {
	font-weight: bold;
	border-bottom: 1rpx solid #d6d6d6;
	padding-bottom: 10rpx;
}

.coupon_item_operate {
	padding-left: 20rpx;
	height: 80rpx;
	border-left: 1rpx solid #d6d6d6;
}

.coupon_item_info {
	width: calc(100% - 100rpx);
}

.coupon_item_rule {
	padding-top: 10rpx;
}

.addCouponBtn {
	width: 90%;
	text-align: center;
	background-color: #fe5512;
	color: #fff;
	line-height: 80rpx;
	border-radius: 999px;
	position: fixed;
	bottom: 30rpx;
	left: 5%;
}
.remind_img{
	width: 32rpx;
	height: 32rpx;
}
.text-FE5512{
	color: #fe5512;
}
.noData{
  text-align: center;
  padding-top: 50rpx
}