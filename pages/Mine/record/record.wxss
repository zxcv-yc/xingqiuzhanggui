.header {
  background: #fe6b10;
  color: #fff;
  font-size: 28rpx;
  padding: 30rpx;
}

page {
  background-color: #f3f3f3;
}

.total-money {
  font-size: 35rpx;
  margin-top: 10rpx;
}

.nav {
  display: flex;
  margin-top: 20rpx;
}

.one {
  flex: 1;
}

.one.zhong {
  text-align: left;
}

.one.you {
  text-align: left;
}

.msg-money {
  font-size: 40rpx;
  margin-top: 10rpx;
  font-weight: bold;
}

.msg-text {
  font-size: 28rpx;
  color: #eee;
}

image {
  width: 100%;
  height: 100%;
}

.list {
  /* padding: 0 30rpx; */
  color: #000;
  font-size: 28rpx;
}

.row {
  overflow: hidden;
  border-bottom: 1rpx solid #e6e6e6;
}

.rleft {
  padding: 15rpx 0;
  float: left;
}

.time {
  color: #999;
  margin-top: 5rpx;
  font-size: 24rpx;
}

.rright {
  float: right;
  padding-top: 30rpx;
  color: #fe5512;
}

.tab_icon_box {
  display: flex;
  align-items: center;
  justify-content: flex-end;
}

.margin_right {
  margin-right: 20rpx;
}
.margin_right_40{
  margin-right: 40rpx
}
.ongoing_box {
  padding: 20rpx;
  background-color: #fff;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.ongoing_left {
  color: #fe5512;
}

.ongoing_right {
  color: #666;
}

.ongoing_left_text {
  font-size: 28rpx;
  padding-left: 20rpx;
  color: #333;
}

.card_round_img {
  width: 60rpx;
  height: 60rpx;
  border-radius: 999rpx;
  background-color: #fe5512;
  margin-right: 20rpx;
  overflow: hidden;
}

.list_card_item {
  margin: 20rpx 0;
  padding: 30rpx;
  background-color: #fff;
}

.card_img {
  width: 120rpx;
  height: 120rpx;
  /* background-color: #d4d4d4; */
  overflow: hidden;
}

.card_content {
  margin-top: 20rpx;
}

.text-999 {
  color: #999;
}

.card_info_box {
  width: calc(100% - 140rpx);
  /* height: 120rpx; */
}

.bottom_bar_btn {
  display: inline-block;
  background-color: #fe5512;
  color: #fff;
  text-align: center;
  padding: 10rpx 40rpx;
  border-radius: 8rpx;
  margin-left: 20rpx;
}

.bottom_bar {
  padding: 20rpx 30rpx;
  background-color: #fff;
  position: fixed;
  bottom: 0;
  width: calc(100% - 60rpx);
}

.text_info {
  font-size: 26rpx;
  color: #666;
}
.fontsize{
  font-size: 40rpx
}