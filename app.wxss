/**公共样式**/
.container {height: 100%;flex-direction: column;align-items: center;justify-content: space-between;box-sizing: border-box;padding-bottom: 100rpx}
.bg{background: #ffbb12!important}  /*主题背景色*/
.tc{color: #ffbb12!important}       /*主题颜色*/
page{border: 0;margin: 0;padding: 0;background:#fff;font-family:SimHei;font-style: 28rpx;color: #333;}
input{outline:none; border:none;list-style: none;}
.disabled{pointer-events: none;color: #999!important}
button::after{border:none;}
button{position:relative;display:block;margin:0;box-sizing:border-box;font-size:32rpx;text-align:center;text-decoration:none;line-height: normal;border-radius:3rpx;-webkit-tap-highlight-color:transparent;overflow:hidden;color:#000000;background-color:#F8F8F8;}
image{height:auto}
/* 授权弹框 */
.auth{width: 100%;height: 100%;position:fixed;top: 0; display: flex;justify-content: center; align-items: center;background-color:rgba(0,0,0,0.5);z-index: 2}
.auth>view{width: 75%;background: #fff; padding: 50rpx 0; border-radius: 10rpx}
.auth>view>.auth-btn{ height: 100rpx; display: flex;justify-content: center; align-items: center;}
.auth>view>.auth-btn>button{background: #179B16;width: 70%;line-height: 60rpx;font-size: 28rpx;height: 60rpx;border-radius: 40rpx;border: none;color: #fff}
.auth>view>.auth-content {text-align: center;width: 90%;margin: 0 auto; line-height: 60rpx;}
.auth>view>.auth-content>.h3{ font-weight: 700;font-size: 32rpx;color: #555;}
.auth>view>.auth-content>.t{ font-size: 25rpx;color: #666;}

/* 没有数据 */
.nodata{height: 80rpx; color: #999; display: block!important; line-height: 80rpx;font-size: 30rpx;text-align: center}

/*遮罩*/
.zhezhao{
	position: fixed;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
	opacity: 0.3;
	background: #000;
}

/* 技术支持 */
.copyright{font-size: 28rpx;color: #ccc;text-align: center;height: 100rpx;line-height: 100rpx}

/* 底部提交按钮 */
.footer {width:100%;display:flex;justify-content:center;align-items:center;height:300rpx}
.footer>button {color:#fff;border-radius:10rpx;width:90%;height:80rpx;line-height:80rpx;}


